{
  "name": "podcast-sharing",
  "version": "1.0.0",
  "main": "src/index.ts",
  "license": "MIT",
  "scripts": {
    "start": "node -r dotenv/config dist/index.js",
    "dev": "ts-node-dev --transpile-only --no-notify -r dotenv/config src/index.ts",
    "build": "yarn generate && tsc",
    "generate": "yarn generate:prisma && yarn generate:graphql",
    "generate:graphql": "graphql-codegen -r dotenv/config",
    "generate:prisma": "prisma generate",
    "update:db": "export $(cat .env | xargs) && prisma db push --preview-feature --force"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-typescript": "^7.12.16",
    "@graphql-codegen/add": "^2.0.2",
    "@graphql-codegen/cli": "^1.17.8",
    "@graphql-codegen/introspection": "^1.17.8",
    "@graphql-codegen/typescript": "^1.19.0",
    "@graphql-codegen/typescript-graphql-request": "^3.0.0",
    "@graphql-codegen/typescript-operations": "^1.17.13",
    "@types/better-sqlite3": "^5.4.1",
    "@types/cheerio": "^0.22.22",
    "@types/date-fns": "^2.6.0",
    "@types/decompress": "^4.2.3",
    "@types/entities": "^1.1.1",
    "@types/html-to-text": "^6.0.0",
    "@types/imagekit": "^3.1.1",
    "@types/jest": "^26.0.20",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/minimist": "^1.2.1",
    "@types/node-fetch": "^2.5.7",
    "@types/oauth": "^0.9.1",
    "@types/react": "^17.0.0",
    "@types/twitter": "^1.7.0",
    "@types/ws": "^7.2.7",
    "babel-jest": "^26.6.1",
    "better-sqlite3": "^7.1.2",
    "jest": "^26.6.3",
    "prettier": "^2.0.5",
    "prisma": "^2.18.0",
    "ts-jest": "^26.5.1",
    "ts-node-dev": "^1.0.0",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "@prisma/client": "^2.18.0",
    "apollo-server-express": "^2.21.0",
    "cheerio": "^1.0.0-rc.3",
    "date-fns": "^2.19.0",
    "decompress": "^4.2.1",
    "decompress-targz": "^4.1.1",
    "dotenv": "^8.2.0",
    "entities": "^2.1.0",
    "env-var": "^6.3.0",
    "graphile-scheduler": "^0.8.0",
    "graphile-worker": "^0.9.0",
    "graphql": "^15.4.0",
    "graphql-request": "^3.4.0",
    "graphql-scalars": "^1.3.0",
    "html-to-text": "^6.0.0",
    "imagekit": "^3.1.2",
    "jsonwebtoken": "^8.5.1",
    "minimist": "^1.2.5",
    "nexus": "^1.0.0",
    "nexus-plugin-prisma": "^0.32.0",
    "node-fetch": "^2.6.1",
    "normalize-url": "^5.3.0",
    "oauth": "^0.9.15",
    "oauth-sign": "^0.9.0",
    "onesignal-node": "^3.2.0",
    "podcastdx-client": "^2.4.1",
    "query-string": "^6.13.2",
    "xmlbuilder2": "^2.2.0"
