    "@graphql-codegen/cli": "^2.3.0",
    "@graphql-codegen/introspection": "^2.1.0",
    "@graphql-codegen/typescript": "^2.4.1",
    "@graphql-codegen/typescript-graphql-request": "^4.3.1",
    "@graphql-codegen/typescript-operations": "^2.2.1",
    "@types/better-sqlite3": "^7.4.1",
    "@types/cheerio": "^0.22.30",
    "@types/date-fns": "^2.6.0",
    "@types/decompress": "^4.2.4",
    "@types/entities": "^2.0.0",
    "@types/html-to-text": "^8.0.1",
    "@types/imagekit": "^3.1.2",
    "@types/jest": "^27.0.3",
    "@types/jsonwebtoken": "^8.5.6",
    "@types/node-fetch": "^2.5.10",
    "@types/oauth": "^0.9.1",
    "@types/react": "^17.0.36",
    "@types/tar": "^6.1.1",
    "@types/turndown": "^5.0.1",
    "@types/twitter": "^1.7.1",
    "babel-jest": "^27.3.1",
    "jest": "^27.3.1",
    "prettier": "^2.4.1",
    "prisma": "^3.5.0",
    "ts-jest": "^27.0.7",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.5.2"
  },
  "dependencies": {
    "@graphile/logger": "^0.2.0",
    "@prisma/client": "^3.5.0",
    "apollo-server-express": "^3.5.0",
    "better-sqlite3": "^7.4.5",
    "cheerio": "^1.0.0-rc.10",
    "date-fns": "^2.26.0",
    "dotenv": "^10.0.0",
    "entities": "^3.0.1",
    "env-var": "^7.1.1",
    "express": "^4.17.1",
    "got": "^11.8.3",
    "graphile-worker": "^0.12.0",
    "graphql": "15",
    "graphql-request": "^3.6.1",
    "graphql-scalars": "^1.13.6",
    "imagekit": "^3.2.3",
    "jsonwebtoken": "^8.5.1",
    "nexus": "^1.1.0",
    "nexus-prisma": "^0.34.0",
    "node-fetch": "^2.6.1",
    "normalize-url": "6",
    "oauth": "^0.9.15",
    "oauth-sign": "^0.9.0",
    "onesignal-node": "^3.3.0",
    "podcastdx-client": "^5.0.0",
    "query-string": "^7.0.1",
