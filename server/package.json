{
  "name": "podcast-sharing",
  "version": "1.0.0",
  "main": "src/index.ts",
  "license": "MIT",
  "scripts": {
    "start": "node -r dotenv/config dist/index.js",
    "dev": "ts-node-dev --transpile-only --no-notify -r dotenv/config src/index.ts",
    "build": "yarn generate && tsc",
    "generate": "yarn generate:prisma && yarn generate:graphql",
    "generate:graphql": "graphql-codegen -r dotenv/config",
    "generate:prisma": "prisma generate",
    "update:db": "export $(cat .env | xargs) && prisma db push --accept-data-loss"
  },
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/preset-env": "^7.14.2",
    "@babel/preset-typescript": "^7.13.0",
    "@graphql-codegen/add": "^2.0.2",
    "@graphql-codegen/cli": "^1.21.4",
    "@graphql-codegen/introspection": "^1.18.2",
    "@graphql-codegen/typescript": "^1.22.0",
    "@graphql-codegen/typescript-graphql-request": "^3.2.0",
    "@graphql-codegen/typescript-operations": "^1.17.16",
    "@types/better-sqlite3": "^5.4.1",
    "@types/cheerio": "^0.22.28",
    "@types/date-fns": "^2.6.0",
    "@types/decompress": "^4.2.3",
    "@types/entities": "^2.0.0",
    "@types/html-to-text": "^6.0.0",
    "@types/imagekit": "^3.1.1",
    "@types/jest": "^26.0.23",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/node-fetch": "^2.5.10",
    "@types/oauth": "^0.9.1",
    "@types/react": "^17.0.6",
    "@types/tar": "^4.0.4",
    "@types/turndown": "^5.0.0",
    "@types/twitter": "^1.7.0",
    "babel-jest": "^26.6.1",
    "jest": "^26.6.3",
    "prettier": "^2.3.0",
    "prisma": "^2.23.0",
    "ts-jest": "^26.5.6",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "@graphile/logger": "^0.2.0",
    "@prisma/client": "^2.23.0",
    "apollo-server-express": "^2.24.1",
    "better-sqlite3": "^7.4.0",
    "cheerio": "^1.0.0-rc.9",
    "date-fns": "^2.21.3",
    "dotenv": "^10.0.0",
    "entities": "^2.2.0",
    "env-var": "^7.0.1",
    "got": "^11.8.2",
    "graphile-worker": "^0.11.1",
    "graphql": "^15.5.0",
    "graphql-request": "^3.4.0",
    "graphql-scalars": "^1.9.3",
    "imagekit": "^3.1.2",
    "jsonwebtoken": "^8.5.1",
    "nexus": "^1.0.0",
    "nexus-plugin-prisma": "^0.35.0",
    "node-fetch": "^2.6.1",
    "normalize-url": "^6.0.0",
    "oauth": "^0.9.15",
    "oauth-sign": "^0.9.0",
    "onesignal-node": "^3.2.1",
    "podcastdx-client": "^5.0.0",
    "query-string": "^7.0.0",
    "tar": "^6.1.0",
    "turndown": "^7.0.0",
    "xmlbuilder2": "^2.4.1"
  }
